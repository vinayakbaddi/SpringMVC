<div class="request-form">
	<div class="sub-head">Request a call back</div>
	<form class="form-query" id="feedbackform">
		<!--server side settings-->
		<div class="request-call-back">
			<div class="rac-label">
				Name <span class="mandatory">*</span>
			</div>
			<div class="rac-value">
				<input type="text" name="name" id="name" placeholder="Full Name"/>
			</div>
			<div class="rac-label">
				Email <span class="mandatory">*</span>
			</div>
			<div class="rac-value">
				<input type="text" name="email" id="email" placeholder="test@abc.com"/>
			</div>
			<div class="rac-label">
				Phone <span class="mandatory">*</span>
			</div>
			<div class="rac-value">
				<input type="text" name="mobile" id="mobile" placeholder="+91-999-999-9999"/>
			</div>
			<div class="none">
				<a onclick="callMe()" class="btn btn-green">Call Me</a>
			</div>
		</div>
	</form>
</div>